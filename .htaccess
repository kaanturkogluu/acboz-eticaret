RewriteEngine On
RewriteBase /acboz-eticaret/

# Force /panel to login page
RewriteRule ^panel/?$ panel/pages/giris.php [L]

# Keep existing files and directories working normally first
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule . - [L]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule . - [L]

# Route /eticaret/giris to login page
RewriteRule ^giris/?$ panel/pages/giris.php [L]

 

