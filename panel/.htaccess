# URL yönlendirmeyi aktif et
 
classes klasörüne erişim yasağı
RewriteEngine On
RewriteBase /eticaret/panel/
RewriteRule ^classes/ - [F,L]
 
# .php uzantısını gizle
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_FILENAME}\.php -f
RewriteRule ^(.+)$ $1.php [L]

# # pages klasörünü kısaltma
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
# RewriteRule ^(anasayfa)$ pages/$1.php [L]
 

RewriteRule ^giris$ pages/giris.php [L]
ErrorDocument 404 /eticaret/panel/pages/404.php
